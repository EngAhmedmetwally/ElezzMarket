
{
  "rules": {
    "orders": {
      "$y": {
        "$m": {
          "$d": {
            ".read": "auth != null",
            ".write": "auth != null"
          }
        }
      }
    },
    "order-lookup": {
        ".read": "auth != null",
        "$orderId": {
            ".write": "auth != null"
        }
    },
    "customer-orders": {
        ".read": "auth != null",
        "$phone": {
            "$orderId": {
                ".write": "auth != null"
            }
        }
    },
    "users": {
        ".read": "auth != null",
        ".write": "auth != null",
        ".indexOn": ["role", "username"]
    },
    "commission-rules": {
      ".read": "auth != null",
      ".write": "auth != null"
    }
  }
}

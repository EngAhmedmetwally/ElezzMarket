{
  "rules": {
    "orders": {
      "$y": {
        "$m": {
          "$d": {
            ".read": true,
            ".write": true
          }
        }
      }
    },
    "order-lookup": {
        ".read": true,
        "$orderId": {
            ".write": true
        }
    },
    "customer-orders": {
        ".read": true,
        "$phone": {
            "$orderId": {
                ".write": true
            }
        }
    },
    "users": {
        ".read": true,
        ".write": true,
        ".indexOn": ["role", "username"]
    },
    "commission-rules": {
      ".read": true,
      ".write": true
    },
    "customers": {
        ".read": true,
        ".write": true
    },
    "products": {
        ".read": true,
        ".write": true
    }
  }
}
